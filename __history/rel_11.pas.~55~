    procedure Timer1Timer(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  Form12: TForm12;

implementation

{$R *.dfm}

procedure TForm12.Timer1Timer(Sender: TObject);
begin
  if Panel1.Visible = False then
    Begin
      Panel1.Visible := True;
      Panel6.Visible := False;
    End
  else
    Begin
    Panel6.Visible := True;
    Panel1.Visible := False;
    End;


end;

end.
