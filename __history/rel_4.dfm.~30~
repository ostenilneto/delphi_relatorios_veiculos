  object ADOQuery3: TADOQuery
    Connection = ADOConnection1
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'select USUARIO, NOME from FAT_VENDEDOR'
      'where FUNCAO = '#39'V'#39' and EMPRESA = 1 and REVENDA = 1 '
      'and USUARIO in '
      '(select UU.USUARIO from GER_USUARIO UU '
      'where UU.GERENTE in (257, 343)) order by NOME')
    Left = 88
    Top = 272
