  object ADOQuery1: TADOQuery
    Connection = ADOConnection1
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'inicio'
        Size = -1
        Value = Null
      end
      item
        Name = 'fim'
        Size = -1
        Value = Null
      end>
    Prepared = True
    SQL.Strings = (
      'SELECT GMO.DES_MOTIVO, '
      'COUNT(*) '
      'FROM CAC_CONTATO CAC '
      'INNER JOIN FAT_VENDEDOR FVE'
      'ON CAC.USUARIO_ENCAMINHADO_ORIGINAL = FVE.USUARIO'
      'INNER JOIN CAC_PROVIDENCIA CPR '
      'ON CAC.EMPRESA = CPR.EMPRESA '
      'AND CAC.REVENDA = CPR.REVENDA '
      'AND CAC.CONTATO = CPR.CONTATO '
      'INNER JOIN GER_MOTIVO GMO '
      'ON (CPR.EMPRESA = GMO.EMPRESA) '
      'AND (CPR.REVENDA = GMO.REVENDA) '
      'AND (CPR.MOTIVO = GMO.MOTIVO)'
      'where ( (CAC.EMPRESA = 1 '
      'and CAC.REVENDA = 1))'
      'and (SUB_TIPO_PROVIDENCIA = '#39'P'#39' '
      'OR CAC.RESULTADO = '#39'P'#39') '
      'AND CAC.DTA_CONTATO >= :inicio '
      'AND CAC.DTA_CONTATO <= :fim'
      
        'and FVE.USUARIO in (select UU.USUARIO from GER_USUARIO UU where ' +
        'UU.GERENTE in (257, 343))  '
      'GROUP by GMO.DES_MOTIVO'
      'order by GMO.DES_MOTIVO')
    Left = 88
    Top = 368
  end
  object Timer1: TTimer
    OnTimer = Timer1Timer
    Left = 16
