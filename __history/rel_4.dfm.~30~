  end
  object ADOQuery2: TADOQuery
    Connection = ADOConnection1
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'inicio'
        Size = -1
        Value = Null
      end
      item
        Name = 'fim'
        Size = -1
        Value = Null
      end
      item
        Name = 'vendedor'
        Size = -1
        Value = Null
      end>
    Prepared = True
    SQL.Strings = (
      'SELECT GMO.DES_MOTIVO AS MOTIVO, '
      'FVE.USUARIO,'
      'FVE.NOME AS VENDEDOR, '
      'COUNT(*) '
      'FROM CAC_CONTATO CAC '
      'INNER JOIN FAT_VENDEDOR FVE'
      'ON CAC.USUARIO_ENCAMINHADO_ORIGINAL = FVE.USUARIO'
      'INNER JOIN CAC_PROVIDENCIA CPR '
      'ON CAC.EMPRESA = CPR.EMPRESA '
      'AND CAC.REVENDA = CPR.REVENDA '
      'AND CAC.CONTATO = CPR.CONTATO '
      'INNER JOIN GER_MOTIVO GMO '
      'ON (CPR.EMPRESA = GMO.EMPRESA) '
      'AND (CPR.REVENDA = GMO.REVENDA) '
      'AND (CPR.MOTIVO = GMO.MOTIVO)'
      'where ( (CAC.EMPRESA = 1 '
      'and CAC.REVENDA = 1)) '
      'AND CAC.DTA_CONTATO >= :inicio '
      'AND CAC.DTA_CONTATO <= :fim'
      
        'and CAC.USUARIO_ENCAMINHADO_ORIGINAL = :vendedor /*USUARIO in (s' +
        'elect UU.USUARIO from GER_USUARIO UU where UU.GERENTE in (257, 3' +
        '43))*/'
      'GROUP by GMO.DES_MOTIVO, FVE.NOME, FVE.USUARIO'
      'order by FVE.NOME')
    Left = 88
    Top = 320
  end
