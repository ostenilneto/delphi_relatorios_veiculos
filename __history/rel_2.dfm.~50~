  object ADOQuery1: TADOQuery
    Connection = ADOConnection1
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'inicio'
        Size = -1
        Value = Null
      end
      item
        Name = 'fim'
        Size = -1
        Value = Null
      end>
    Prepared = True
    SQL.Strings = (
      'SELECT nf.REVENDA,'
      '       nf.CONTATO,'
      '       nf.NUMERO_NOTA_FISCAL AS NUMERO_NF, '
      '       nf.SERIE_NOTA_FISCAL AS SERIE,'
      '      nf.DEPARTAMENTO,'
      '       nf.TIPO_TRANSACAO AS TRANSACAO,'
      '       nf.DTA_ENTRADA_SAIDA AS DATA,'
      '       nf.CLIENTE,'
      '      cli.NOME AS NOME,'
      '      cli.uf_entrega as ESTADO'
      '  '
      ''
      'FROM fat_movimento_capa nf, fat_cliente cli'
      ''
      'WHERE nf.DTA_ENTRADA_SAIDA  between :inicio and :fim'
      
        'AND nf.TIPO_TRANSACAO in ('#39'O21'#39', '#39'P21'#39', '#39'P24'#39', '#39'P31'#39', '#39'P51'#39', '#39'V0' +
        '7'#39', '#39'V20'#39', '#39'V21'#39', '#39'U07'#39', '#39'U21'#39')'
      'AND nf.status = '#39'F'#39
      'AND nf.CLIENTE = cli.CLIENTE'
      'AND  cli.uf_entrega <> '#39'MG'#39
      ''
      
        'ORDER BY  nf.DTA_ENTRADA_SAIDA, nf.REVENDA, nf.TIPO_TRANSACAO, n' +
        'f.CONTATO')
    Left = 83
    Top = 222
  end
