  object ADOQuery1: TADOQuery
    Connection = ADOConnection1
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'inicio'
        Size = -1
        Value = Null
      end
      item
        Name = 'fim'
        Size = -1
        Value = Null
      end
      item
        Name = 'departamento'
        Size = -1
        Value = Null
      end
      item
        Name = 'departamento2'
        Size = -1
        Value = Null
      end>
    Prepared = True
    SQL.Strings = (
      'SELECT GMO.DES_MOTIVO "MOTIVO",'
      'sum((case when FVE.USUARIO = 468 then 1 else 0 end)) "ALLINE",'
      'sum((case when FVE.USUARIO = 232 then 1 else 0 end)) "ALTAMIRO",'
      
        'sum((case when FVE.USUARIO = 501 then 1 else 0 end)) "ANA KARLA"' +
        ','
      'sum((case when FVE.USUARIO = 218 then 1 else 0 end)) "DAYANE",'
      'sum((case when FVE.USUARIO = 243 then 1 else 0 end)) "DENIS",'
      'sum((case when FVE.USUARIO = 375 then 1 else 0 end)) "JO'#195'O",'
      'sum((case when FVE.USUARIO = 116 then 1 else 0 end)) "LARA",'
      'sum((case when FVE.USUARIO = 315 then 1 else 0 end)) "MARCO", '
      'sum((case when FVE.USUARIO = 257 then 1 else 0 end)) "MARIO",'
      'sum((case when FVE.USUARIO = 534 then 1 else 0 end)) "RODRIGO",'
      'sum((case when FVE.USUARIO = 505 then 1 else 0 end)) "WESLAINE",'
      'count(*) total'
      'FROM GER_DEPARTAMENTO DEP, CAC_CONTATO CAC'
      'INNER JOIN FAT_VENDEDOR FVE'
      'ON CAC.USUARIO_ENCAMINHADO_ORIGINAL = FVE.USUARIO'
      'INNER JOIN CAC_PROVIDENCIA CPR '
      'ON CAC.EMPRESA = CPR.EMPRESA '
      'AND CAC.REVENDA = CPR.REVENDA '
      'AND CAC.CONTATO = CPR.CONTATO '
      'INNER JOIN GER_MOTIVO GMO '
      'ON (CPR.EMPRESA = GMO.EMPRESA) '
      'AND (CPR.REVENDA = GMO.REVENDA) '
      'AND (CPR.MOTIVO = GMO.MOTIVO)'
      'where ( (CAC.EMPRESA = 1 '
      'and CAC.REVENDA = 1))'
      'and (SUB_TIPO_PROVIDENCIA = '#39'P'#39' '
      'OR CAC.RESULTADO = '#39'P'#39') '
      
        'AND CAC.DTA_CONTATO between To_Date(:inicio,'#39'dd/mm/yyyy'#39') and To' +
        '_Date(:fim, '#39'dd/mm/yyyy'#39')'
      'and FVE.DEPARTAMENTO = DEP.DEPARTAMENTO'
      'and DEP.DEPARTAMENTO in (:departamento, :departamento2)'
      'GROUP by GMO.DES_MOTIVO'
      'order by GMO.DES_MOTIVO')
    Left = 88
    Top = 368
  end
  object Timer1: TTimer
    OnTimer = Timer1Timer
    Left = 16
    Top = 192
  end
  object ADOQuery2: TADOQuery
    Connection = ADOConnection1
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'inicio'
        Size = -1
        Value = Null
      end
      item
        Name = 'fim'
        Size = -1
        Value = Null
      end
      item
        Name = 'vendedor'
        Size = -1
        Value = Null
      end>
    Prepared = True
    SQL.Strings = (
      'select CAC.REVENDA, CAC.CONTATO, '
      'GU1.NOME USUARIO_ABRIU, GU2.NOME USUARIO_ENCAMINHADO, '
      'FC.NOME CLIENTE, '
      '(select MD.DES_MODELO '
      'from CAC_INTERESSE INTE, VEI_MODELO MD '
      'where INTE.EMPRESA = CAC.EMPRESA '
      'and INTE.REVENDA = CAC.REVENDA '
      'and INTE.CONTATO = CAC.CONTATO '
      'and INTE.EMPRESA = MD.EMPRESA '
      'and INTE.MODELO = MD.MODELO '
      'and INTE.INTERESSE = (select max(INTE2.INTERESSE) '
      'from CAC_INTERESSE INTE2 '
      'where INTE2.EMPRESA = INTE.EMPRESA '
      'and INTE2.REVENDA = INTE.REVENDA '
      'and INTE2.CONTATO = INTE.CONTATO)) MODELO, '
      'CAC.DTA_CONTATO, CAC.DTA_NOVO_CONTATO, '
      '(select CP1.DES_PROVIDENCIA '
      'from CAC_PROVIDENCIA CP1 '
      'where CP1.EMPRESA = CAC.EMPRESA '
      'and CP1.REVENDA = CAC.REVENDA '
      'and CP1.CONTATO = CAC.CONTATO '
      'and CP1.PROVIDENCIA = (select max(CP2.PROVIDENCIA) '
      'from CAC_PROVIDENCIA CP2 '
      'where CP2.EMPRESA = CP1.EMPRESA '
      'and CP2.REVENDA = CP1.REVENDA '
      'and CP2.CONTATO = CP1.CONTATO)) COMENTARIO '
      'from GER_USUARIO GU1, GER_USUARIO GU2, CAC_CONTATO CAC '
      'inner join CAC_FORMA_CONTATO CFC '
      
        'on (CAC.EMPRESA = CFC.EMPRESA and CAC.FORMA_CONTATO = CFC.FORMA_' +
        'CONTATO) '
      'inner join FAT_CLIENTE FC on (CAC.CLIENTE = FC.CLIENTE) '
      'inner join FAT_VENDEDOR FV on (CAC.EMPRESA = FV.EMPRESA '
      
        'and CAC.REVENDA = FV.REVENDA and CAC.USUARIO_ENCAMINHADO = FV.US' +
        'UARIO) '
      'left join CAC_INTERESSE CI '
      'on (CAC.EMPRESA = CI.EMPRESA and CAC.REVENDA = CI.REVENDA '
      'and CAC.CONTATO = CI.CONTATO) '
      'left join CAC_PROVIDENCIA CP on (CAC.EMPRESA = CP.EMPRESA '
      'and CAC.REVENDA = CP.REVENDA and CAC.CONTATO = CP.CONTATO '
      'and CP.TIPO_PROVIDENCIA = '#39'F'#39') '
      'where CFC.TIPO_FORMA_CONTATO in (1,2,3,4) '
      'and CAC.ORIGEM = '#39'V'#39' '
      'and GU1.USUARIO = CAC.USUARIO_ABRIU '
      'and GU2.USUARIO = CAC.USUARIO_ENCAMINHADO '
      'and CAC.EMPRESA = 1 and CAC.REVENDA = 1 '
      
        'and CP.DTA_PROVIDENCIA between To_Date(:inicio,'#39'dd/mm/yyyy'#39') and' +
        ' To_Date(:fim, '#39'dd/mm/yyyy'#39') '
      'and CP.PROVIDENCIA = (select max(providencia) '
      'from CAC_PROVIDENCIA CP1 '
      'where CP1.EMPRESA = CAC.EMPRESA '
      'AND CP1.REVENDA = CAC.REVENDA '
      'AND CP1.CONTATO = CAC.CONTATO) '
      'and CAC.RESULTADO = '#39'P'#39' '
      'and CAC.SITUACAO = '#39'F'#39' '
      'and FV.vendedor = :vendedor'
      'GROUP BY CAC.CONTATO, CAC.SITUACAO, '
      'CAC.USUARIO_ABRIU, CAC.USUARIO_ENCAMINHADO, '
      'FC.NOME, CAC.DTA_CONTATO, CAC.DTA_NOVO_CONTATO, '
      'GU1.NOME, GU2.NOME, CAC.EMPRESA, CAC.REVENDA')
    Left = 88
    Top = 320
  end
  object DataSource2: TDataSource
    DataSet = ADOQuery2
    Left = 160
    Top = 320
  end
  object DataSource3: TDataSource
    DataSet = ADOQuery3
