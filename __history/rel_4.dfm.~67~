  object ADOQuery3: TADOQuery
    Connection = ADOConnection1
    CursorType = ctStatic
    DataSource = DataSource1
    Parameters = <
      item
        Name = 'gerente1'
        Size = -1
        Value = Null
      end>
    SQL.Strings = (
      'select USUARIO, NOME'
      'from GER_USUARIO '
      'where (GERENTE = :gerente1'
      'or GERENTE = gerente2)'
      'AND ATIVO = '#39'S'#39
      'AND GRUPO IN (107,102,112,37)'
      'ORDER BY NOME')
    Left = 88
    Top = 272
    object ADOQuery3VENDEDOR: TIntegerField
      FieldName = 'VENDEDOR'
    end
    object ADOQuery3NOME: TStringField
      FieldName = 'NOME'
      Size = 50
    end
  end
