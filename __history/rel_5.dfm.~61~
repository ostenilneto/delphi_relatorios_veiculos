  object ADOQuery3: TADOQuery
    Active = True
    Connection = ADOConnection1
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'SELECT OT.ORIGEM_TRAFEGO, OT.DES_ORIGEM_TRAFEGO '
      'FROM CAC_ORIGEM_TRAFEGO OT, '
      'CAC_ORIGEM_TRAFEGO_DEPT OTD, '
      'GER_DEPARTAMENTO GD '
      'WHERE OTD.EMPRESA = 1 '
      'AND OTD.REVENDA = 1 '
      'AND OTD.DEPARTAMENTO = 100 '
      'AND OT.ORIGEM_TRAFEGO = OTD.ORIGEM_TRAFEGO '
      'AND GD.EMPRESA = OTD.EMPRESA '
      'AND GD.REVENDA = OTD.REVENDA '
      'AND GD.DEPARTAMENTO = OTD.DEPARTAMENTO')
    Left = 88
    Top = 272
    object ADOQuery3ORIGEM_TRAFEGO: TIntegerField
      FieldName = 'ORIGEM_TRAFEGO'
    end
    object ADOQuery3DES_ORIGEM_TRAFEGO: TStringField
      FieldName = 'DES_ORIGEM_TRAFEGO'
      Size = 30
    end
  end
