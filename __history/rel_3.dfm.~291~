  object ADOQuery1: TADOQuery
    Connection = ADOConnection1
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'inicio'
        Size = -1
        Value = Null
      end
      item
        Name = 'fim'
        Size = -1
        Value = Null
      end
      item
        Name = 'gerente2'
        Size = -1
        Value = Null
      end
      item
        Name = 'gerente1'
        Size = -1
        Value = Null
      end>
    Prepared = True
    SQL.Strings = (
      'select FVE.NOME VENDEDOR, '
      
        #9'(select count(*) from CAC_CONTATO CC, VEI_PROPOSTA PROP, VEI_NE' +
        'GOCIACAO NEG where CC.EMPRESA = FVE.EMPRESA and CC.REVENDA = FVE' +
        '.REVENDA and CC.USUARIO_ENCAMINHADO_ORIGINAL = FVE.USUARIO and C' +
        'C.EMPRESA = PROP.EMPRESA and CC.REVENDA = PROP.REVENDA and CC.CO' +
        'NTATO = PROP.CONTATO and CC.AGENDA_PESQUISA not in (select CAP.A' +
        'GENDA_PESQUISA from CAC_AGENDA_PESQUISA CAP where CAP.EMPRESA = ' +
        'CC.EMPRESA and CAP.REVENDA = CC.REVENDA and CAP.AGENDA_PESQUISA ' +
        '= CC.AGENDA_PESQUISA and CAP.TIPO_PESQUISA = 2) and PROP.EMPRESA' +
        ' = NEG.EMPRESA and PROP.REVENDA = NEG.REVENDA and PROP.PROPOSTA ' +
        '= NEG.PROPOSTA and PROP.NEGOCIACAO_FINAL = NEG.NEGOCIACAO and PR' +
        'OP.SITUACAO in ('#39'7'#39','#39'9'#39') and TRUNC(NEG.DTA_APR_GERENCIAL) >=  :i' +
        'nicio  and TRUNC(NEG.DTA_APR_GERENCIAL) <= :fim) VENDAS'
      ' from FAT_VENDEDOR FVE, GER_DEPARTAMENTO DEP '
      #9'where FVE.EMPRESA = DEP.EMPRESA '
      #9'and FVE.REVENDA = DEP.REVENDA '
      #9'and FVE.DEPARTAMENTO = DEP.DEPARTAMENTO '
      #9'and DEP.IDENTIFICACAO = '#39'V'#39' '
      #9'and FVE.FUNCAO = '#39'V'#39' '
      #9'and FVE.EMPRESA = 1 '
      #9'and FVE.REVENDA = 1 '
      #9'and FVE.USUARIO in (select UU.USUARIO from GER_USUARIO UU'
      ' where UU.GERENTE = :gerente2'
      'or UU.GERENTE = :gerente1) '
      #9'and length(FVE.COD_VW) > 0 '
      #9'order by FVE.NOME')
    Left = 88
    Top = 368
  end
