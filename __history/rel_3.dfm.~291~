  object ADOQuery2: TADOQuery
    Connection = ADOConnection1
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'inicio'
        Size = -1
        Value = Null
      end
      item
        Name = 'fim'
        Size = -1
        Value = Null
      end
      item
        Name = 'inicio2'
        Size = -1
        Value = Null
      end
      item
        Name = 'fim2'
        Size = -1
        Value = Null
      end
      item
        Name = 'gerente2'
        Size = -1
        Value = Null
      end
      item
        Name = 'gerente1'
        Size = -1
        Value = Null
      end
      item
        Name = 'vendedor'
        Size = -1
        Value = Null
      end>
    Prepared = True
    SQL.Strings = (
      
        'select CC.CONTATO as "Contato", CC.DTA_FECHAMENTO as "Data do fe' +
        'chamento", CC.CLIENTE as "C'#243'digo Cliente", CLI.NOME as "Nome cli' +
        'ente", PROP.PROPOSTA as "Proposta", PROP.VEICULO as "C'#243'digo do v' +
        'e'#237'culo"'
      
        'from CAC_CONTATO CC, FAT_CLIENTE CLI,VEI_PROPOSTA PROP, FAT_VEND' +
        'EDOR FVE, VEI_NEGOCIACAO NEG '
      'where CC.EMPRESA = FVE.EMPRESA '
      'and CC.REVENDA = FVE.REVENDA '
      'and CC.USUARIO_ENCAMINHADO_ORIGINAL = FVE.USUARIO'
      'and CC.EMPRESA = PROP.EMPRESA '
      'and CC.REVENDA = PROP.REVENDA '
      'and CC.CONTATO = PROP.CONTATO'
      'and CC.CLIENTE = CLI.CLIENTE '
      'and PROP.EMPRESA = NEG.EMPRESA '
      'and PROP.REVENDA = NEG.REVENDA '
      'and PROP.PROPOSTA = NEG.PROPOSTA '
      'and TRUNC(NEG.DTA_APR_GERENCIAL) >=  :inicio  '
      'and TRUNC(NEG.DTA_APR_GERENCIAL) <= :fim'
      'and PROP.SITUACAO in ('#39'7'#39','#39'9'#39') '
      'and TRUNC(NEG.DTA_APR_GERENCIAL) >= :inicio2 '
      'and TRUNC(NEG.DTA_APR_GERENCIAL) <= :fim2'
      'and FVE.USUARIO in (select UU.USUARIO from GER_USUARIO UU'
      ' where UU.GERENTE = :gerente2'
      'or UU.GERENTE = :gerente1) '
      'and FVE.VENDEDOR = :vendedor'
      'order by 2')
    Left = 88
    Top = 320
  end
