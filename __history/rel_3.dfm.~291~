  object ADOQuery3: TADOQuery
    Connection = ADOConnection1
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'gerente2'
        Size = -1
        Value = Null
      end
      item
        Name = 'gerente1'
        Size = -1
        Value = Null
      end>
    SQL.Strings = (
      'select VENDEDOR, NOME '
      'from FAT_VENDEDOR'#9
      'where FUNCAO = '#39'V'#39' '
      'and EMPRESA = 1 '
      'and REVENDA = 1 '
      'and USUARIO in (select UU.USUARIO'
      'from GER_USUARIO UU '
      'where UU.GERENTE = :gerente2'
      'or UU.GERENTE = :gerente1)'
      'order by NOME')
    Left = 88
    Top = 272
    object ADOQuery3VENDEDOR: TIntegerField
      FieldName = 'VENDEDOR'
    end
    object ADOQuery3NOME: TStringField
      FieldName = 'NOME'
      Size = 50
    end
  end
